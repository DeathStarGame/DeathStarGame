{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Both",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}/run/deathstar",
				"--extensionDevelopmentPath=${workspaceFolder}/run/mult"]
		},
		{
			"name": "Run Death Star",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}/run/deathstar"]
		},
		{
			"name": "Run Mult",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}/run/mult"]
		}
	],
	"compounds": [
    {
      "name": "Run Compound",
      "configurations": ["Run Death Star", "Run Mult"]
    }
  ]
}
